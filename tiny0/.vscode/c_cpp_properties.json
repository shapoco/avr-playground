        {
            "name": "Linux",
            "includePath": [
                "${AVR_GCC_PATH}/avr/include",
                "${workspaceFolder}/common/include"
            ],
            "defines": [
                "__AVR_ATtiny1604__",
                "I2C_INCLUDE_IMPL",
                "AQM0802_INCLUDE_IMPL",
                "DELAY_INCLUDE_IMPL"
